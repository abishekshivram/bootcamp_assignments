S_0x57fec0b10d90 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x57fec0b10f20 .scope module, "tb_counter" "tb_counter" 3 3;
 .timescale -9 -10;
P_0x57fec0ae8b50 .param/l "COUNT_WD" 1 3 5, +C4<00000000000000000000000000001000>;
v0x57fec0b26650_0 .var "i_clk", 0 0;
