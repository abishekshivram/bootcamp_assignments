S_0x57fec0ad4cf0 .scope module, "dut" "counter" 3 10, 4 3 0, S_0x57fec0b10f20;
 .timescale -9 -10;
    .port_info 0 /INPUT 1 "i_clk";
    .port_info 1 /INPUT 1 "i_rstb";
    .port_info 2 /INPUT 1 "i_tm_reset";
    .port_info 3 /INPUT 1 "i_tm_direction";
    .port_info 4 /OUTPUT 8 "o_count";
P_0x57fec0ad4ed0 .param/l "COUNT_WD" 0 4 5, +C4<00000000000000000000000000001000>;
v0x57fec0ad5000_0 .net "i_clk", 0 0, v0x57fec0b26650_0;  1 drivers
v0x57fec0b25f90_0 .net "i_rstb", 0 0, v0x57fec0b26710_0;  1 drivers
v0x57fec0b26050_0 .net "i_tm_direction", 0 0, v0x57fec0b267e0_0;  1 drivers
v0x57fec0b26120_0 .net "i_tm_reset", 0 0, v0x57fec0b268e0_0;  1 drivers
v0x57fec0b261e0_0 .var "o_count", 7 0;
v0x57fec0b26310_0 .var "r_count", 7 0;
v0x57fec0b263f0_0 .var "r_count_inc", 7 0;
v0x57fec0b264d0_0 .var "r_count_next", 7 0;
E_0x57fec0b0e370/0 .event negedge, v0x57fec0b25f90_0;
E_0x57fec0b0e370/1 .event posedge, v0x57fec0ad5000_0;
E_0x57fec0b0e370 .event/or E_0x57fec0b0e370/0, E_0x57fec0b0e370/1;
E_0x57fec0b0e910 .event anyedge, v0x57fec0b26050_0, v0x57fec0b26120_0, v0x57fec0b26310_0, v0x57fec0b263f0_0;
    .scope S_0x57fec0ad4cf0;
T_0 ;
    %wait E_0x57fec0b0e910;
    %load/vec4 v0x57fec0b26050_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.0, 8;
    %pushi/vec4 255, 0, 8;
    %jmp/1 T_0.1, 8;
T_0.0 ; End of true expr.
    %pushi/vec4 1, 0, 8;
    %jmp/0 T_0.1, 8;
 ; End of false expr.
    %blend;
T_0.1;
    %store/vec4 v0x57fec0b263f0_0, 0, 8;
    %load/vec4 v0x57fec0b26120_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.2, 8;
    %pushi/vec4 0, 0, 8;
    %jmp/1 T_0.3, 8;
T_0.2 ; End of true expr.
    %load/vec4 v0x57fec0b26310_0;
    %load/vec4 v0x57fec0b263f0_0;
    %add;
    %jmp/0 T_0.3, 8;
 ; End of false expr.
    %blend;
T_0.3;
    %store/vec4 v0x57fec0b264d0_0, 0, 8;
    %load/vec4 v0x57fec0b26310_0;
    %store/vec4 v0x57fec0b261e0_0, 0, 8;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x57fec0ad4cf0;
T_1 ;
    %wait E_0x57fec0b0e370;
    %load/vec4 v0x57fec0b25f90_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x57fec0b26310_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x57fec0b264d0_0;
    %assign/vec4 v0x57fec0b26310_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x57fec0b10f20;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x57fec0b26650_0;
    %inv;
    %store/vec4 v0x57fec0b26650_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x57fec0b10f20;
T_3 ;
    %vpi_call/w 3 26 "$dumpfile", "./results/counter_ivlog.vcd" {0 0 0};
    %vpi_call/w 3 27 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x57fec0b10f20 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57fec0b26650_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57fec0b26710_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57fec0b26710_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57fec0b26710_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57fec0b268e0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57fec0b267e0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57fec0b268e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57fec0b268e0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57fec0b267e0_0, 0, 1;
